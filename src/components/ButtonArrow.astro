---
import IconBase from "./svg/IconBase.astro";
import IconArrow from "./svg/IconArrow.astro";

type Props = {
  title?: string;
  class?: string;
  href: string;
};

const { title, class: classes, href } = Astro.props;
---

<a class:list={[classes]} href={href} title={title}>
  <IconBase class="rotate-180"><IconArrow /></IconBase>
</a>
